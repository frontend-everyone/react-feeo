.col-sm-1-5, .col-md-1-5, .col-lg-1-5 {
  min-height: 1px;
  padding-left: 15px;
  padding-right: 15px;
  position: relative;
  float: left;
  width: 50%;
}

@media (min-width: 768px) {
  .col-sm-1-5 {
    width: 20%
  }
}

@media (min-width: 992px) {
  .col-md-1-5 {
    width: 20%;
  }
}

@media (min-width: 1200px) {
  .col-lg-1-5 {
    width: 20%;
  }
}



